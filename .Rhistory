library(ggplot2)
library(tidyverse)
library(dplyr)
sleep_data <- read.csv("sleep75.csv")
sleep_data <- read.csv("sleep75.csv");
average_sleep_age <- sleep_data %>%
group_by(age) %>%
summarize(avg_sleep = mean(sleep))
ggplot(average_sleep_age, aes(x = age, y =  (avg_sleep/60)/7)) +
geom_line(color = "red") +
labs(title = "Correlation between Age and Average Hours Slept Per Day",
x = "Age",
y = "Average Hours Slept Per Day")
library(ggplot2)
library(tidyverse)
library(dplyr)
average_sleep_expr <- sleep_data %>%
group_by(exper) %>%
summarize(avg_sleep = mean(sleep))
chart <- ggplot(average_sleep_expr, aes(x = exper, y =   (avg_sleep/60)/7)) +
geom_line(color = "blue") +
labs(title = "Correlation between Exeperience and Hours Worked Per Day",
x = "Experience",
y = "Hours Worked Per Day" )
print(chart)
